<%- include('header.ejs') %>

<style>
    .center{
        text-align: center;
    }


</style>
<body>
<h1 class="center"><%= subject %> Questions</h1>


<form action="/findQuestion" method="post">

    <div class="inner center">
            <% for(var i=0; i<docs.length; i++) {%>
                <div><b>User:</b> <%= docs[i].username %></div>
                <button  name="questionSelect" type='submit' value='<%= docs[i].id %>' ><b>Question:</b> <%= docs[i].question %></button>


            <br><br><br>
            <% } %>






    </div>

</form>



</body>














<%- include('footer.ejs') %>